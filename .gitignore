src-tauri/bundled/mariadb/**/share/
src-tauri/bundled/mariadb/**/support-files/
src-tauri/bundled/mariadb/**/data/
# Keep templates and small helpers
!src-tauri/bundled/mariadb/**/*.template
!src-tauri/bundled/mariadb/**/bin/resolveip
!src-tauri/bundled/mariadb/**/bin/my_print_defaults
!src-tauri/bundled/mariadb/**/bin/mariadb-install-db

# Allow placeholder so CI can keep an empty bundled dir when real bundles are not present
!src-tauri/bundled/.gitkeep

